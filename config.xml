<?xml version='1.0' encoding='utf-8'?>
<widget id="br.fib.lp5.prova2" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Prova 2bim</name>
    <description>
        Prova 2bimestre.
    </description>
    <author email="patriciasoramos@gmail.com">
        Patricia de Souza Ramos
    </author>
    <preference name="DisallowOverscroll" value="true" />
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <platform name="android">
        <preference name="StatusBarBackgroundColor" value="#e81272" />
    </platform>
    <platform name="ios">
        <preference name="StatusBarBackgroundColor" value="#e81272" />
    </platform>
    <engine name="android" spec="~5.2.2" />
    <engine name="ios" spec="~4.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="~2.2.0" />
</widget>
